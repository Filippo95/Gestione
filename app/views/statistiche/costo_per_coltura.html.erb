    <script>
$( document ).ready(function() {


            $.ajax({    url: "costo_per_coltura.json",
                        dataType: "json",
                        timeout: 800000,
                        cache: true}).done(function (o) {
                        console.log(o)
                        for (var i = 0; i < o.length; i++)
                            {
                                $('#tab').append('<tr><td>' +o[i][0]+ '</td><td>'  +o[i][1]+ '</td><td>'  +o[i][2]+ '</td></tr> ')
                            }
                            $('#data').DataTable();

                        });
   


           
                        	// Example Data

		var data = <%=raw @data%>
$.plot('#placeholder', data, {
    series: {
        pie: {
            show: true
        }
    }
});

		
			
			
